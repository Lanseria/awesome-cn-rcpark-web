{
  "name": "awesome-cn-rcpark",
  "private": true,
  "packageManager": "pnpm@6.32.6",
  "scripts": {
    "fetch": "git clone -b rc-park https://github.com/Lanseria/awesome-cn-data cafe 2> /dev/null || git -C cafe pull",
    "data": "node ./scripts/process.mjs",
    "postinstall": "npm run fetch && npm run data",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify/vue": "^3.2.1",
    "@mapbox/mapbox-gl-language": "^1.0.0",
    "@vueuse/core": "^8.2.5",
    "emitt": "^1.3.4",
    "fuse.js": "^6.5.3",
    "mapbox-gl": "^2.8.0",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@iconify-json/carbon": "^1.1.3",
    "@types/node": "^17.0.23",
    "@unocss/reset": "^0.31.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.19",
    "eslint": "^8.13.0",
    "jsdom": "^19.0.0",
    "pnpm": "^6.32.6",
    "typescript": "^4.6.3",
    "unocss": "^0.31.0",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-pages": "^0.22.0",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.33.9"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
